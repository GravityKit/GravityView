    "startDate": {
      "default": "",
      "type": "string"
    },
    "startDateType": {
      "default": "static",
      "type": "string"
    },
    "endDate": {
      "default": "",
      "type": "string"
    },
    "endDateType": {
      "default": "static",
      "type": "string"
    },
    "classValue": {
      "default": "",
      "type": "string"
    },
    "offset": {
      "default": 0,
      "type": "number"
    },
    "singleTitle": {
      "default": "",
      "type": "string"
