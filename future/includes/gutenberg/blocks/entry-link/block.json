    "entryId": {
      "default": "",
      "type": "string"
    },
    "postId": {
      "default": 0,
      "type": "number"
    },
    "action": {
      "default": "read",
      "type": "string"
    },
    "returnFormat": {
      "default": "html",
      "type": "string"
    },
    "linkAtts": {
      "default": "",
      "type": "string"
    },
    "fieldValues": {
      "default": "",
      "type": "string"
    },
    "content": {
      "default": "",
      "type": "string"
    },
    "previewBlock": {
      "type": "boolean",
      "default": false
    },
    "previewAsShortcode": {
      "type": "boolean",
      "default": false
    },
    "showPreviewImage": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {
    "attributes": {
      "showPreviewImage": true
    }
  }
